{
  "name": "mern_eccom",
  "version": "1.0.0",
  "description": "mern eccom app",
  "main": "server.js",
  "engines": {
    "node": "14.13.0",
    "npm": "6.14.8"
  },
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm run start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd frontend && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NateMail/merneccom.git"
  },
  "author": "Nate Mailhiot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NateMail/merneccom/issues"
  },
  "homepage": "https://propshopmern.herokuapp.com",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
